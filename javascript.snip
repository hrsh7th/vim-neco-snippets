snippet function
alias   func
options word
  function ${1:#:function_name}(${2}) {
    ${0:TARGET}
  }

snippet proto
options word
  prototype.${2:#:method_name} = function (${3}) {
    ${0:TARGET}
  };

snippet f
options word
  function (${1}) {${0:TARGET}}

snippet sett
options head
  setTimeout(${0:TARGET}, 10)

snippet jqp
options head
  $.proxy(${0:TARGET}, this)

