snippet proto
	prototype.${1} = function (${2}) {
	  ${0}
	};

snippet f
	function () {${0}}

snippet if
	if (${1:true}) {${0}}

snippet ife
	if (${1:true}) {${2}} else {${0}}

snippet ifif
	if (${1:true}) {${2}} else if (${3:true})  {${0}}

snippet timeout
	setTimeout(function() {${0}}${2:}, ${1:10});

snippet jqp
	$.proxy(${0}, this)

